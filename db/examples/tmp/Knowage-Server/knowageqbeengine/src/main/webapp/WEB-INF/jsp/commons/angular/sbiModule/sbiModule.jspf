<script>

	var  app = angular.module('sbiModule',['ngSanitize']);
	
	app.config(function($mdDateLocaleProvider) {
      
        $mdDateLocaleProvider.parseDate = function(dateString,format) {
        	if(!format) format = 'L';
            var m = moment(dateString, format,'<%=locale.getLanguage()%>_<%=locale.getCountry()%>', true);
            return m.isValid() ? m.toDate() : new Date(NaN);
        };
        $mdDateLocaleProvider.formatDate = function(date,format) {
        	if(!format) format = 'L';
            return moment(date).locale('<%=locale.getLanguage()%>_<%=locale.getCountry()%>').format(format);
        };
    });
	
</script>

